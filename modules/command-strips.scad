SMALL_COMMAND_STRIP_X = 16.0;
SMALL_COMMAND_STRIP_Y = 47.0;
SMALL_COMMAND_STRIP_Z = 0.8; // this is without the tabs to cover before installed

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

module SmallCommandStrip()
{
    r = SMALL_COMMAND_STRIP_X / 2;

    union()
    {
        cube([ SMALL_COMMAND_STRIP_X, SMALL_COMMAND_STRIP_Y - r, SMALL_COMMAND_STRIP_Z ]);

        translate([ r, SMALL_COMMAND_STRIP_Y - r, 0 ])
            cylinder(
                r = r,
                h = SMALL_COMMAND_STRIP_Z
            );
    }
}

